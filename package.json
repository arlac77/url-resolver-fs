{
  "name": "url-resolver-fs",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./src/module.mjs",
  "exports": {
    ".": "./src/module.mjs"
  },
  "description": "resolves urls and provides fs like access",
  "keywords": [
    "resolver",
    "url",
    "url-resolver-fs"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs && ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "btoa": "^1.2.1",
    "http-proxy-agent": "^7.0.0",
    "https-proxy-agent": "^7.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "c8": "^8.0.1",
    "documentation": "^14.0.2",
    "semantic-release": "^21.1.1"
  },
  "engines": {
    "node": ">=14.18.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/url-resolver-fs"
  },
  "bugs": {
    "url": "https://github.com/arlac77/url-resolver-fs/issues"
  },
  "homepage": "https://github.com/arlac77/url-resolver-fs#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
