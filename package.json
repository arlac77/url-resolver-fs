{
  "name": "uri-resolver",
  "version": "0.0.0-semantic-release",
  "description": "uri resolver",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "cover": "npm run build && node --harmony ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "test": "npm run build && mocha --recursive tests/*",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=dist/index.js -- src/index.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/uri-resolver.git"
  },
  "keywords": [
    "uri",
    "resolver"
  ],
  "author": "Markus Felten <markus.felten@gmx.de>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/uri-resolver/issues"
  },
  "homepage": "https://github.com/arlac77/uri-resolver#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "3.1.0",
    "rollup": "^0.36.1",
    "semantic-release": "^6.3.1"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "https-proxy-agent": "^1.0.0",
    "node-fetch": "^1.6.3",
    "registry-mixin": "^2.1.0"
  },
  "engines": {
    "node": ">=6.2"
  }
}
