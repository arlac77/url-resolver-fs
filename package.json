{
  "name": "url-resolver-fs",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/module.mjs",
  "exports": {
    ".": "./src/module.mjs"
  },
  "description": "resolves urls and provides fs like access",
  "keywords": [
    "resolver",
    "url",
    "url-resolver-fs"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/module.mjs --section=API && documentation readme ./src/module.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint src/module.mjs && documentation lint ./src/module.mjs"
  },
  "dependencies": {
    "btoa": "^1.2.1",
    "http-proxy-agent": "^4.0.0",
    "https-proxy-agent": "^5.0.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "c8": "^7.4.0",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.3"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/url-resolver-fs.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/url-resolver-fs/issues"
  },
  "homepage": "https://github.com/arlac77/url-resolver-fs#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
