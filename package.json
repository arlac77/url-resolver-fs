{
  "name": "uri-resolver",
  "version": "0.0.0-semantic-release",
  "description": "resolves uris and provides fs like access",
  "main": "dist/index.js",
  "module": "src/index.js",
  "scripts": {
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "build": "node_modules/rollup/bin/rollup --output=dist/index.js -c -- src/index.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/uri-resolver.git"
  },
  "keywords": [
    "uri",
    "resolver"
  ],
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/arlac77/uri-resolver/issues"
  },
  "homepage": "https://github.com/arlac77/uri-resolver#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "mocha": "^3.2.0",
    "rollup": "^0.36.4",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "semantic-release": "^6.3.3"
  },
  "dependencies": {
    "btoa": "^1.1.2",
    "https-proxy-agent": "^1.0.0",
    "node-fetch": "^1.6.3",
    "registry-mixin": "^2.1.3"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}